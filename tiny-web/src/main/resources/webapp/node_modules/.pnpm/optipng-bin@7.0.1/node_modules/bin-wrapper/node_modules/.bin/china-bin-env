#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/robotto/Documents/projects/frontend/tiny-web/node_modules/.pnpm/bin-wrapper-china@0.1.0/node_modules/bin-wrapper-china/node_modules:/Users/robotto/Documents/projects/frontend/tiny-web/node_modules/.pnpm/bin-wrapper-china@0.1.0/node_modules:/Users/robotto/Documents/projects/frontend/tiny-web/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/robotto/Documents/projects/frontend/tiny-web/node_modules/.pnpm/bin-wrapper-china@0.1.0/node_modules/bin-wrapper-china/node_modules:/Users/robotto/Documents/projects/frontend/tiny-web/node_modules/.pnpm/bin-wrapper-china@0.1.0/node_modules:/Users/robotto/Documents/projects/frontend/tiny-web/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../inject-env.js" "$@"
else
  exec node  "$basedir/../../inject-env.js" "$@"
fi
